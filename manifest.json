{
  "name": "ZombieBuster",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Delete all zombies from X",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [{
    "matches": ["https://x.com/*"],
    "js": [
      "scripts/main.js"
    ]
  }]
}